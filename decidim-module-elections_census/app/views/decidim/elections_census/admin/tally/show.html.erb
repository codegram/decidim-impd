<div class="card">
  <div class="card-divider">
    <h2 class="card-title"><%= t ".export_votes" %></h2>
  </div>
  <div class="card-section">
    <div class="row column">
      <% if can_export_votes? %>
        <%= link_to t(".export_votes"), tally_export_votes_path, class: "button" %>
      <% else %>
        <%= t(".cant_export_votes_yet") %>
      <% end %>
    </div>
  </div>
</div>
